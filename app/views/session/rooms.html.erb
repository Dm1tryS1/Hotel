<!-- Header Section Begin -->
    <header class="header-section">
        <div class="container-fluid">
            <div class="inner-header">
                <div class="logo">
                    <%= link_to image_tag("/img/logo.png"), root_path%>
                </div>
                <div class="nav-right">
                    <%= link_to t('.makeapplication'), new_proposal_path,  class: "primary-btn"%>
                    <%= link_to t('.admin'), session_acc_path, class: "primary-btn"%>
                    <%= link_to t('.applications'), proposals_path, class: "primary-btn"%>
                </div>
                <nav class="main-menu mobile-menu">
                    <ul>
                        <li><%= link_to t('.home'), session_index_path%></a></li>
                        <li><%= link_to t('.about'), session_aboutus_path%></li>
                        <li class="active"><%= link_to t('.rooms'), session_rooms_path%></li>
                        <li><%= link_to t('.contact'), session_contact_path%></li>
                    </ul>
                </nav>
                <div id="mobile-menu-wrap"></div>
            </div>
        </div>
    </header>
<!-- Header End -->

<!-- Hero Area Section Begin -->
    <div class="hero-area set-bg other-page" data-setbg="/img/about_bg.jpg">
    </div>
<!-- Hero Area Section End -->

